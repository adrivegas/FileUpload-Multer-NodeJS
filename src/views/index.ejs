<body>


    <hr>
    <div class="container">
        <div class="row">
            <div class="col text-center">
                <h1>Envío de archivos</h1>
            </div>
        </div>
    </div>
    <hr>
    
    
    
    <div class="container">
        <div class="row">
            <div class="col mt-5 text-center">
    
                <form action="http://localhost:3000/uploads" method="POST" enctype="multipart/form-data">
    
                    <div class="form-group cualquiera">
                        <div class="col">
                             <i class="far fa-id-card fa-3x"></i>
                             <p>Cédula</p>
                        </div>
                        <div class="col">
                            <input type="file" name="image" class="form-control-file">
                        </div>
                        <div class="col">
                            <button type="submit" class="btn btn-success btn-s">Enviar</button>
                        </div>
                    </div>
   
                </form>

                <form action="http://localhost:3000/uploads" method="POST" enctype="multipart/form-data">
    
                    <div class="form-group cualquiera">
                        <div class="col">
                            <i class="far fa-id-card fa-3x"></i>
                            <p>RUT</p>
                        </div>
                        <div class="col">
                            <input type="file" name="image" class="form-control-file">
                        </div>
                        <div class="col">
                            <button type="submit" class="btn btn-success btn-s">Enviar</button>
                        </div>
                    </div>
  
                </form>



                <form action="http://localhost:3000/uploads" method="POST" enctype="multipart/form-data">
    
                    <div class="form-group cualquiera">
                        <div class="col">
                            <i class="far fa-file-alt fa-3x"></i>
                            <p>Cámara de comercio</p>
                        </div>
                        <div class="col">
                            <input type="file" name="image" class="form-control-file">
                        </div>
                        <div class="col">
                            <button type="submit" class="btn btn-success btn-s">Enviar</button>
                        </div>
                    </div>
   
                </form>

    
    
            </div>
        </div>
    </div>


